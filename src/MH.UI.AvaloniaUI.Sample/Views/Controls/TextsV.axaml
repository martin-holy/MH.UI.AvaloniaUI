<DataTemplates
  xmlns="https://github.com/avaloniaui"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:vmc="clr-namespace:MH.UI.AvaloniaUI.Sample.ViewModels.Controls"
  xmlns:c="clr-namespace:MH.UI.AvaloniaUI.Controls;assembly=MH.UI.AvaloniaUI"
  xmlns:ap="clr-namespace:MH.UI.AvaloniaUI.AttachedProperties;assembly=MH.UI.AvaloniaUI">

  <DataTemplate DataType="{x:Type vmc:TextsVM}">
    <StackPanel Orientation="Vertical">
      <TextBox Text="TextBox" Margin="4"/>
      <TextBox Text="TextBox" Margin="4" IsInactiveSelectionHighlightEnabled="True"/>
      <TextBlock Text="TextBlock" Margin="4"/>
      <Label Content="Label"/>
      <c:IconTextBlock ap:Icon.Res="IconBug" ap:Text.Text="Sample" Margin="2" Shadow="None"/>
      <c:IconTextBlock ap:Icon.Res="IconBug" ap:Text.Text="Sample" Margin="2" Shadow="Icon"/>
      <c:IconTextBlock ap:Icon.Res="IconBug" ap:Text.Text="Sample" Margin="2" Shadow="Text"/>
      <c:IconTextBlock ap:Icon.Res="IconBug" ap:Text.Text="Sample" Margin="2"/>
      <c:IconTextBlock ap:Icon.Res="IconBug" ap:Text.Text="Sample" Margin="2" Classes="bordersRoundDark"/>
    </StackPanel>
  </DataTemplate>
</DataTemplates>