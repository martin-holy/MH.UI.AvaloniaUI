<ResourceDictionary
  xmlns="https://github.com/avaloniaui"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:c="clr-namespace:MH.UI.AvaloniaUI.Controls">

  <ControlTheme x:Key="{x:Type c:SlidePanelHost}" TargetType="c:SlidePanelHost">
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="c:SlidePanelHost">
          <ContentPresenter Content="{Binding ViewModel.Content, RelativeSource={RelativeSource TemplatedParent}, Mode=OneWay}"/>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </ControlTheme>
</ResourceDictionary>